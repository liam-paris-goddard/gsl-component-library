{
  "name": "gsl-component-library",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/gsl-component-library/gsl-component-library.esm.js",
  "exports": {
    ".": {
      "import": "./dist/gsl-component-library/gsl-component-library.esm.js",
      "require": "./dist/gsl-component-library/gsl-component-library.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "npm run build-storybook && storybook dev -p 6006",
    "build-storybook": "stencil build &&  storybook build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@stencil/core": "^4.27.1",
    "@stencil/sass": "^3.2.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/html": "^8.6.12",
    "@storybook/html-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.14",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "npm": "^11.3.0",
    "puppeteer": "^24.3.0",
    "sass": "^1.86.3",
    "storybook": "^8.6.12",
    "storybook-addon-stencil": "^0.2.3"
  },
  "license": "MIT",
  "dependencies": {
    "@carbon/icon-helpers": "^10.57.0",
    "@carbon/icons": "^11.58.0",
    "@carbon/styles": "^1.80.0",
    "@carbon/type": "^11.38.0",
    "@carbon/web-components": "^2.28.0"
  }
}
